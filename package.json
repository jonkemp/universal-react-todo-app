{
  "name": "universal-react-todo-app",
  "version": "1.0.0",
  "description": "React.js Universal Todo App",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production node_modules/.bin/webpack -p",
    "dev": "node_modules/.bin/webpack -d --watch --progress",
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' src/app.js",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "test": "mocha --compilers js:babel-core/register",
    "selenium": "java -jar selenium-server-standalone-2.53.1.jar",
    "webdriver": "./node_modules/.bin/wdio wdio.conf.js"
  },
  "author": "Jonathan Kemp",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-register": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.5.0",
    "http-server": "^0.9.0",
    "mocha": "^3.0.2",
    "wdio-mocha-framework": "^0.4.3",
    "wdio-spec-reporter": "0.0.3",
    "webdriverio": "^4.2.15",
    "webpack": "^1.13.2"
  }
}
